il, password);\n  };\n\n  ////sign user out\n  const signOut = () => auth.signOut();\n\n  useEffect(() => {\n    const unsubscribe = auth.onAuthStateChanged((user) => {\n      setCurrentUser(user);\n      setLoading(false);\n      if (!user) {\n        return;\n      } else {\n        const userdisplayName = user.displayName;\n        if (userdisplayName === null) {\n          return;\n        }\n        const names = userdisplayName.split(\",\");\n        setFirstN(names[1]);\n        setLastN(names[2]);\n        setInitials(names[0]);\n      }\n    });\n\n    return unsubscribe;\n  }, [profileUpdate]);\n\n  const value = {\n    currentUser,\n    signUp,\n    signIn,\n    signOut,\n    firstN,\n    lastN,\n    initials,\n  };\n\n  return (\n    <AuthContext.Provider value={value}>\n      {!loading && children}\n    </AuthContext.Provider>\n  );\n}\n","/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/components/PrivateRoutes.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/NotFoundPage.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":11,"column":10,"nodeType":"111","messageId":"112","endLine":11,"endColumn":15},{"ruleId":"113","severity":1,"message":"114","line":19,"column":6,"nodeType":"115","endLine":19,"endColumn":20,"suggestions":"116"},{"ruleId":"109","severity":1,"message":"117","line":29,"column":13,"nodeType":"111","messageId":"112","endLine":29,"endColumn":23},{"ruleId":"109","severity":1,"message":"118","line":1,"column":8,"nodeType":"111","messageId":"112","endLine":1,"endColumn":13},{"ruleId":"109","severity":1,"message":"118","line":1,"column":8,"nodeType":"111","messageId":"112","endLine":1,"endColumn":13},{"ruleId":"105","replacedBy":"119"},{"ruleId":"107","replacedBy":"120"},{"ruleId":"109","severity":1,"message":"121","line":45,"column":15,"nodeType":"111","messageId":"112","endLine":45,"endColumn":35},"no