nitials(initials);\n  }, [nameInitials]);\n\n  const handleSingOut = async () => {\n    try {\n      await signOut();\n      history.push(\"/signin\");\n    } catch {\n      setError(\"failed to log out\");\n    }\n  };\n  return (\n    <>\n      <Nav.Item>\n        <LinkContainer to=\"/create\">\n          <Nav.Link>New Project</Nav.Link>\n        </LinkContainer>\n      </Nav.Item>\n      <Nav.Item>\n        <Button\n          style={{ textDecoration: \"none\" }}\n          variant=\"link\"\n          onClick={handleSingOut}\n        >\n          Log Out\n        </Button>\n      </Nav.Item>\n      <Nav.Item>\n        <LinkContainer to=\"/\">\n          <Nav.Link className=\"badge  rounded-circle bg-primary py-2 mt-2 mx-3 \">\n            {initials}\n          </Nav.Link>\n        </LinkContainer>\n      </Nav.Item>\n    </>\n  );\n};\n\nexport default SignedIn;\n","/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/components/layouts/SignedOut.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/DashBoardPage.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/components/Notifications.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/components/ProjectList.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/components/ProjectSummary.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/ProjectDetailsPage.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/SignInPage.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/SignUpPage.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/project-app/src/pages/CreateProjectPage.js",["101"],"import React, { useState } from \"react\";\nimport { Container, Form, Button, Row, Col } from \"react-bootstrap\";\nimport { useMutation } from \"react-query\";\nimport {